<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title><%- name %></title>
  <link rel="stylesheet" href="docs.css" />
</head>
<body>
  <div id="root">
    <div class="layout" :class="{ 'is-main-full': isMainFull }">
      <header class="layout-header">
        <nav class="navbar">
          <a href="index.html" class="navbar-title"><%- name %></a>
        </nav>
      </header>
      <main class="layout-main">
        <div class="content">
          <%- content %>
        </div>
      </main>
      <aside class="layout-sidebar">
        <nav class="sidebar">
          <div class="sidebar-content">
            <% Object.keys(sideMenu).forEach(function(key) { %>
              <h2 class="sidebar-group"><%= key %></h2>
              <ul class="sidebar-menu">
                <% sideMenu[key].forEach(function(menu) { %>
                  <li class="sidebar-link">
                    <a href="<%= menu.path %>"><%= menu.title %></a>
                  </li>
                <% }); %>
              </ul>
            <% }); %>
          </div>
        </nav>
      </aside>
      <footer class="layout-footer footer">
        Copyright Â© 2020-present AR advanced technology, Inc
      </footer>
      <button class="layout-toggler" @click="toggleLayout">
        <svg v-if="isMainFull" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd" d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708z"/>
          <path fill-rule="evenodd" d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708z"/>
        </svg>
        <svg v-else viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd" d="M8.354 1.646a.5.5 0 0 1 0 .708L2.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0z"/>
          <path fill-rule="evenodd" d="M12.354 1.646a.5.5 0 0 1 0 .708L6.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0z"/>
        </svg>
      </button>
    </div>
  </div>
  <script src="docs.js"></script>
</body>
</html>
